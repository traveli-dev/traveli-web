    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "babel-loader": "^8.2.4",
    "eslint": "^8.0.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
