    "firebase": "^9.8.2",
    "framer-motion": "^3.1.1",
    "next": "^12.1.0",
    "next-auth": "^4.3.1",
    "prettier": "^2.6.0",
    "qrcode.react": "^3.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.30.0",
